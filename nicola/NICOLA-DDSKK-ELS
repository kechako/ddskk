;;; -*-Emacs-Lisp-*-

;; NICOLA-DDSKK-ELS: list of NICOLA-DDSKK modules to install

;;; Code:

(require 'path-util)
(and (module-installed-p 'easymenu)
     (require 'easymenu))

(setq nicola-ddskk-modules
      '(nicola-ddskk-autoloads
	skk-106-jis
	skk-kanagaki
	skk-kanagaki-menu
	skk-kanagaki-util
	skk-nicola-dcomp
	skk-nicola-dvorak
	skk-nicola-jis
	skk-nicola-us
	skk-nicola
	skk-oasys
	skk-omelet-dvorak
	skk-omelet-jis
	skk-omelet-us))

(and (featurep 'xemacs)
     (setq nicola-ddskk-modules
	   (delq 'nicola-ddskk-autoloads nicola-ddskk-modules)))

(and (featurep 'easymenu)
     (or (fboundp 'easy-menu-add-item)
	 (setq nicola-ddskk-modules
	       (nconc nicola-ddskk-modules '(skk-kanagaki-menu-oe)))))

;;; NICOLA-DDSKK-ELS ends here
